{
  "name": "yogpt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently \"vite\" \"wait-on http://localhost:8000 && cross-env NODE_ENV=development electron .\"",
    "start-mini-server": "node miniServer.js",
    "dist": "electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@material-tailwind/react": "^2.1.9",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.15.19",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@stripe/react-stripe-js": "^2.7.2",
    "@stripe/stripe-js": "^4.0.0",
    "axios": "^1.7.2",
    "chart.js": "^4.4.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "diskusage": "^1.2.0",
    "express": "^4.19.2",
    "figlet": "^1.7.0",
    "file-saver": "^2.0.5",
    "firebase": "^10.10.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "moment": "^2.30.1",
    "pidusage": "^3.0.2",
    "plotly.js": "^2.32.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.23.1",
    "react-spinners": "^0.13.8",
    "react-terminal-ui": "^1.3.0",
    "react-toastify": "^10.0.5",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.12.7",
    "sweetalert2": "^11.6.13",
    "terminal-in-react": "^4.3.1",
    "tw-elements": "^2.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^25.9.8",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.4.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.yourapp.id",
    "files": [
      "dist/**/*",
      "main.cjs",
      "miniServer.js",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "miniServer.js",
        "to": "miniServer.js"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not < 0.2%",
    "not op_mini all",
    "Edge >= 18"
  ]
}
